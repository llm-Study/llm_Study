{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "myvue-install": "cnpm install --prefix myvue",
    "myvue": "cnpm start --prefix myvue",
    "start": "cnpm ./bin/www",
    "server": "./node_modules/.bin/nodemon bin/www",
    "dev": "concurrently \" cnpm run myvue  \" \" cnpm run server\""
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "debug": "^2.6.3",
    "koa": "^2.2.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "^0.0.16",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "mongoose": "^5.5.2",
    "pug": "^2.0.0-rc.1"
  },
  "devDependencies": {
    "nodemon": "^1.8.1"
  }
}
